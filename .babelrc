{
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true,
            "modules": false
          }
        ]
      ],
      "plugins": ["@babel/plugin-proposal-class-properties"]
    },
    "es": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true,
            "modules": false
          }
        ]
      ],
      "plugins": ["@babel/plugin-proposal-class-properties"]
    },
    "lib": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true
          }
        ]
      ],
      "plugins": ["@babel/plugin-proposal-class-properties"]
    },
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true,
            "modules": false
          }
        ]
      ],
      "plugins": ["@babel/plugin-proposal-class-properties"]
    },
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true
          }
        ],
        "@babel/preset-react"
      ],
      "plugins": ["@babel/plugin-proposal-class-properties"]
    }
  }
}
